<?php

/**
 * @file
 * Install, update and uninstall functions for the sponsors starter module.
 */

/**
 * Implements hook_uninstall().
 */
function sponsors_starter_uninstall() {
  \Drupal::configFactory()->getEditable('sponsors.sponsor_type.sponsors')->delete();
  \Drupal::configFactory()->getEditable('views.view.sponsors')->delete();
}
